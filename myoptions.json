{
    "animelist": {
        "username": "sadru"
    },
    "filter": {
        "spoilers": false
    },
    "download": {
        "filename": "%(short_anime_title)s-%(type)s.%(filetype)s",
        "audio_folder": "anime_themes/audio",
        "video_folder": "anime_themes/video",
        "no_redownload": true,
        "timeout": 15,
        "sort": "year",
        "coverart": {
            "resolution":1,
            "folder":"anime_themes/coverarts"
        },
        "compression": {
            "root_dir": "anime_themes",
            "base_name":"animethemes",
            "format":"tar",
            "base_dir":"audio"
        }
    },
    "statuses": [1,2],
    "ffmpeg": "ffmpeg"
}